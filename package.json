{
  "name": "giraffe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0-rc.0",
    "@material-ui/icons": "^4.0.0-rc.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "apollo-boost": "^0.4.3",
    "apollo-link-schema": "^1.2.3",
    "clean-deep": "^3.0.2",
    "clsx": "^1.0.4",
    "d3": "^5.9.7",
    "firebase": "^6.0.4",
    "flow-bin": "^0.98.1",
    "fontfaceobserver": "^2.1.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "husky": "^2.4.0",
    "lint-staged": "^8.1.7",
    "lodash": "^4.17.13",
    "multi-metagen": "^2.6.0",
    "normalize-wheel": "^1.0.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "protobufjs": "^6.8.8",
    "protocol-buffers-encodings": "^1.1.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-loadable": "^5.5.0",
    "react-localize-redux": "^3.5.3",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "roboto-fontface": "^0.10.0",
    "simplebar-react": "^1.1.0",
    "typeface-roboto-condensed": "0.0.54",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "start": "npm run generate && cross-env REACT_APP_VERSION=$npm_package_version react-scripts start",
    "build": "npm run generate && cross-env REACT_APP_VERSION=$npm_package_version react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proto": "cross-env-shell pbjs --es6 -t json src/proto/*.json src/proto/DataTypes.proto -o src/generated/generated-$npm_package_version-schema.json",
    "preproto": "cross-env-shell pbjs --es6 -t proto3 src/proto/*.json -o src/proto/Generated.proto",
    "generate-schemas": "cross-env-shell metagen --filter=json src/generated es6WithFileExtension",
    "generate-images": "npm run generate-images-items && npm run generate-images-machines",
    "generate-images-items": "cross-env-shell metagen --filter=png src/apps/Graph/libraries/SGLib/images/items es6WithFileExtension",
    "generate-images-machines": "cross-env-shell metagen --filter=png src/apps/Graph/libraries/SGLib/images/machines es6WithFileExtension",
    "generate": "npm run proto && npm run generate-schemas && npm run generate-images"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "jest-canvas-mock": "^2.1.0",
    "protocol-buffers": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
